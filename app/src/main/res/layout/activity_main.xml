<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_tab_host"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.dingding.activity.MainActivity">


    <FrameLayout
        android:id="@+id/realtabcontent"
        android:layout_width="match_parent"
        android:layout_height="0dip"
        android:layout_weight="1">
    </FrameLayout>
    
    <com.example.dingding.ben.view.fragmentTabHost.FragmentTabHost
        android:id="@+id/fth_tabhost"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#99eeeeee"
        android:paddingTop="10px"
        ></com.example.dingding.ben.view.fragmentTabHost.FragmentTabHost>

</LinearLayout>
